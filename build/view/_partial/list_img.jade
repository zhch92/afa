mixin imglist(listID)
	.afa-m-imgshow(id=listID)
		ul.list
			-for(var i=0;i<4;i++)
				li.item
					.img
						img(src="../img/list_img.jpg", alt="")
					if block
						block
					else
						.user
							img.tou(src="../img/tou.jpg", alt="")
							a(href="#")
								i.icon.icon-like
								| 34
							a(href="#")
								i.icon.icon-comment
								| 23
